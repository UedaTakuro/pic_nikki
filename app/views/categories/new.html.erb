<div class="container">
  <div class="row">
    <div class="col-sm-12 col-md-8 col-lg-5 px-5 px-sm-0 mx-auto">
      <h2>Category new</h2>
      <%= render 'layouts/error', obj: @category %>

      <%= form_with model: @category do |f| %>
        <h4>Name</h4>
        <%= f.text_field :name %>
  
        <%= f.submit 'Create' %>
      <% end %>
    </div>
  </div>
</div>